{
  "version": 3,
  "routes": [
    {
      "src": "/icon.png",
      "dest": "/icon.png"
    },
    {
      "src": "/logo.webp",
      "dest": "/logo.webp"
    },
    {
      "src": "/og-image.jpg",
      "dest": "/og-image.jpg"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
